generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  ADIM
  USER
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
  password String
  role Role
}


